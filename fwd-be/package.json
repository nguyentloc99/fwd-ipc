{
    "name": "fwd-be",
    "version": "1.0.0",
    "description": "Fwd BFF",
    "main": "index.js",
    "scripts": {
        "build:local": "gulp build:local",
        "build:staging": "gulp build:staging",
        "build:dev": "gulp build:dev",
        "build:production": "gulp build:production",
        "build:test": "gulp build:test",
        "start": "NODE_ENV=local nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
        "start:window": "SET NODE_ENV=local && nodemon --watch src/**/*.ts --ignore src/**/*.spec.ts --exec ts-node src/index.ts",
        "start:basic": "NODE_ENV=local node --require ts-node/register --watch src/index.ts",
        "local": "npm run build:local && (tsc -w & nodemon dist/index.js local)",
        "local:window": "npm run build:local --window && (tsc -w & nodemon dist/index.js local)",
        "staging": "npm run build:staging && (tsc -w & nodemon dist/index.js staging)",
        "dev": "npm run build:dev && (tsc -w & nodemon dist/index.js dev)",
        "production": "npm run build:production && (tsc -w & nodemon dist/index.js production)",
        "document": "gulp document-watch",
        "test": "gulp test",
        "test:cov": "gulp test --reports=true",
        "verify-lint": "tslint -c tslint.json -p tsconfig.json"
    },
    "pre-commit": [
        "verify-lint"
    ],
    "repository": {
        "type": "git",
        "url": "todo"
